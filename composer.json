{
    "name": "laravel-enso/commentsmanager",
    "description": "Comments Manager for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "commentsmanager",
        "comments-manager",
        "comments"
    ],
    "homepage": "https://github.com/laravel-enso/CommentsManager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel-enso/core": "1.3.*",
        "laravel-enso/trackwho": "1.3.*",
        "laravel-enso/vueadminlte": "1.3.*",
        "laravel-enso/structuremanager": "1.3.*"
    },
    "require-dev": {
        "laravel-enso/testhelper": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\CommentsManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\CommentsManager\\CommentsServiceProvider",
                "LaravelEnso\\CommentsManager\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}
